  <script type="text/javascript">
      jQuery( document ).ready( function ()
      {
          
          // TT 25624, Fix for IE 8: target element: txtAbstract
          // issue: IE8 has some bugs with max-width/height combined with overflow: auto/scroll. It renders page blank.
          var $txtAbstract = jQuery( '#txtAbstract' );
          if ( $txtAbstract.length )
          {
              if ( document.documentMode ) // only IE 8.0+ supports documentMode
              {
                  if ( document.documentMode == 8 )
                  {
                      if ( $txtAbstract.height() > 280 )
                          $txtAbstract.css( 'height', '280px' );

                      // maintain proposed max height while resizing the window
                      jQuery( window ).resize( function ( event )
                      {
                          $txtAbstract.css( 'height', 'auto' );
                          if ( $txtAbstract.height() > 280 )
                              $txtAbstract.css( 'height', '280px' );
                      } );

                  } else
                  { // all IE > IE 8.0
                      $txtAbstract.css( 'max-height', '280px' );
                  }
              } else
              { // all non-IE, & IE < IE 8.0
                  $txtAbstract.css( 'max-height', '280px' );
              }
          }

          // assign window.focus to handler
          $(window).focus(WindowFocusEvent);

          jQuery( function ()
          {
              var link = jQuery( 'a#FundingInfoLink' );

              link.hover( function ()
              {
                  // put the hover div back to its default position
                  resetFundingHoverLink();

                  var fundingHoverDiv = jQuery( 'div#FundingHover' );
                  var fundingInfoLink = jQuery( 'a#FundingInfoLink' );

                  // if the hover div will go off the bottom of the screen, put it on the bottom of the screen.
                  if ( fundingInfoLink.offset().top + fundingHoverDiv.innerHeight() > jQuery(document).height() )
                  {
                      fundingHoverDiv.css( "top", "auto" );
                      fundingHoverDiv.css( "bottom", 1 );
                      fundingHoverDiv.css( "position", "fixed" );
                  }
                  fundingHoverDiv.show();
              }, function ()
              {
                  jQuery( 'div#FundingHover' ).hide();
              } );

              resetFundingHoverLink();
          } );

          function resetFundingHoverLink()
          {
              var link = jQuery( 'a#FundingInfoLink' );

              if (link.length > 0) {    // bugfix 27206 20140619 BBD 
                  jQuery('div#FundingHover').css({
                      left: link.position().left + link.width() + 10,
                      top: link.position().top - 10,
                      bottom: "auto",
                      position: "absolute"
                  });
              }
          }
      } );
      //Bugfix 30368 WP 20160804
      // TT#31416 20170516 VP, corrected jQuery selection by Id
      function checkSavedState(id) {
          var $hdnAmdWasChecked = $('#hdnAMDWasChecked_' + id);
          if ($hdnAmdWasChecked.val() === 'True') {
              return;
          }
          
          if($hdnAmdWasChecked.val() === 'Save'){
              $hdnAmdWasChecked.val('');
          } else {
              $hdnAmdWasChecked.val('Save');
          }
      }

      // 14.0-10 2016-09-19 MattM
      function validatePage(target)
      {
          if (typeof Page_ClientValidate == "function")
          {
              Page_ClientValidate();
              if (!Page_IsValid)
              {
                  var message = "";
                  for (i = 0; i < Page_Validators.length; i++)
                  {
                      if (!Page_Validators[i].isvalid)
                      {
                          message += "- " + Page_Validators[i].errormessage + "<br/>";
                      }
                  }
                  $("#errorDialog")[0].showDialog(message);
                  return false;
              }
          }

          var pageValid = true;

          if (typeof validateGlobalMSID == 'function')
          {
              pageValid = validateGlobalMSID(target);
          }

          if (typeof validateDOI == 'function')
          {
              pageValid = pageValid && validateDOI(target);
          }

          if (pageValid)
          {
              __doPostBack(target);
              jQuery('#btnSave1, #btnSave2, #btnSaveAndClose1, #btnSaveAndClose2').prop("disabled", true);
          }

          return false;
      }
  </script>
    

