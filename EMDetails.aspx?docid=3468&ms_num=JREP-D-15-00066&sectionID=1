	function abstractEditWarning(fieldName)
	{
	    $("#errorDialog")[0].showDialog(FormatString("Warning\x3a The {0} cannot be edited from Manuscript Details - any changes will not be saved. \x28Editors with \x27Edit Submission\x27 privileges may edit the abstract by clicking the \x27Edit Submission\x27 action link.\x29", [fieldName]));
	}
	function authorCommentEditWarning()
	{
	    $("#errorDialog")[0].showDialog("Warning\x3a Author Comments cannot be edited from  Manuscript Details - any changes will not be saved. \x28Editors with \x27Edit Submission\x27 privileges may edit Author Comments by clicking the \x27Edit Submission\x27 action link\x29.");
	}
	function getHelpTitle()
