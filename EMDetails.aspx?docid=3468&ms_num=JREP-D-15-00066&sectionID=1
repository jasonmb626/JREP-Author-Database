	}

    // handles window.focus for showing/hiding the CIF results link table cell, if necessary
	function WindowFocusEvent()
	{
	    var $CIFResultsLinkElements = $('.CIFResultsLinkElement');
	    var $CIFTopLink = $('a[href*="CIF"]');
	    CIFResultsLinkHandler_Initialize(CIFResultsLinkType_Submission, 3468);
	    var resultCount = CIFResultsLinkHandler_GetStatusCount();

	    if (resultCount > 0)
	    {
	        $CIFResultsLinkElements.show();
	        $CIFTopLink.show();
	    }
	    else
	    {
	        $CIFResultsLinkElements.hide();
	        $CIFTopLink.hide();
	    }
	}

