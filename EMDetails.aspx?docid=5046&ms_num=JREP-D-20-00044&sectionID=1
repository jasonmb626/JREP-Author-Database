                $(document).ready(function()
                {
                    $requestCompleteDialog = $("#requestCompleteDialog").dialog({
                        title: 'Request Completed',
                        modal : true,
                        width: 450,
                        autoOpen : false,
                        resizable : false,
                        buttons : {
                                'OK' : function() { 
                                    $requestCompleteDialog.dialog("close"); 
                                     }
                                 
                            }
                        ,
                        draggable : false,
                        dialogClass: 'show-titlebar no-close',
                        closeOnEscape: true
                    });
                    $("#requestCompleteDialog").siblings('.ui-dialog-titlebar').prepend("<span class=''></span>");

                    $("#requestCompleteDialog")[0].showDialog = function(text, okAction, cancelAction)
                                        {
                                            if (okAction != null)
                                            {
                                                $requestCompleteDialog.dialog('option', 'buttons', {
                                                    'OK': function()   {
                                                                            $requestCompleteDialog.dialog("close");
                                                                            okAction();
                                                                        }
                                                     
                                                });
                                            }
                                            if (text != null) $(this)[0].innerHTML = text
