<script src="/v16.1/WebResource.axd?d=3kX9Bgis_7FHLTST_GcdfELX8xTB1fmeF-g7wqgiGJsbg9jGqFFjmyg_mpeGE_zbmcs9zEYEE-XfntTOdpI9Xd71UJ2CrSusYxgWvOvn-9NHjjZ9VNEtWdyl1T-JoeocouRIfBM4YnyISL_PKboKIemEnBXjqC3gBBeKv0--ut72fZp-0&amp;t=637230787414127425" type="text/javascript"></script>
<script src="/v16.1/WebResource.axd?d=uOzx-EYL6CN2J6PFudEwgLNmbfYHy5SoHO3HYz_6RkC5AgaRvQCt2SfvFMnzfeZoqGuVafAQG6w8YEDZH-rMOmGwsz7SJ-xeXFchqcpXma2eDeP_4ezkdh9yoZ_x_UO-ddmQomIOwbrABMq2pZRkTr1-9D7gIO3K30r1o6MdJIyEk4DGHzUOw0TaDcNwCT1zzDRTpfnBvd-bJQ-Xw0RY5e-Zd1A1&amp;t=637230787414127425" type="text/javascript"></script>
<script type="text/javascript">
//<![CDATA[

	function validateDateForbidPast(source, args)
	{
		validateDate(source, args, true);
	}

	function validateDateAllowPast(source, args)
	{
		validateDate(source, args, false);
	}

	function validateDate(source, args, mustBeFuture)
	{
		var txtBox = document.getElementById(source.controltovalidate);

        if (txtBox == null)
		{
            return;
        }

        var inputString = args.Value.replace(' ','');

        var inputDate = new Date(inputString);

        var errorPrefix = '';
		var hiddenFieldID = txtBox.id.replace('inputDate','ErrMsgPrefix');
		var hiddenField = document.getElementById(hiddenFieldID);
		if (hiddenField != null)
		{
		        errorPrefix = hiddenField.value;
		}

        var showErrorPrefixOnly = false;
		var prefixOnlyHiddenFieldID = txtBox.id.replace('inputDate','ShowErrPrefixOnly');
		var prefixOnlyHiddenField = document.getElementById(prefixOnlyHiddenFieldID);
		if (prefixOnlyHiddenField != null)
		{
		        showErrorPrefixOnly = (prefixOnlyHiddenField.value == '1');
		}

        var controlToValidateChanged = (txtBox.value != txtBox.defaultValue);

		if (controlToValidateChanged)
		{

			var patternMatch = inputString.match(/^\d{1,2}\/\d{1,2}\/\d{4}$/);

			if (patternMatch == null)
			{
			        args.IsValid = false;
			        setErrorMessage(source, errorPrefix, showErrorPrefixOnly, 'Invalid date format please enter as follows\x3a \u201cmm\x2fdd\x2fyyyy\u201d');
			        txtBox.focus();
			        txtBox.select();
			        return;
			}

			if (inputDate == null)
			{
			        args.IsValid = false;
			        setErrorMessage(source, errorPrefix, showErrorPrefixOnly, 'Input is not a valid date');
			        txtBox.focus();
			        txtBox.select();
			        return;
			}

			var year = inputDate.getFullYear();
			var month = inputDate.getMonth() + 1;
			var day = inputDate.getDate();

			if (isNaN(year) || isNaN(month)|| isNaN(day))
			{
			    args.IsValid = false;
			    setErrorMessage(source, errorPrefix, showErrorPrefixOnly, 'Input is not a valid date');
			    txtBox.focus();
			    txtBox.select();
			    return;
			}

            var dateObject = isDate(inputString);
			if (dateObject == null)
			{
			        args.IsValid = false;
			        setErrorMessage(source, errorPrefix, showErrorPrefixOnly, 'Input is not a valid date');
			        txtBox.focus();
			        txtBox.select();
			        return;
			}


		    var currDate = new Date();
		    currDate.setHours(0);
		    currDate.setMinutes(0);
		    currDate.setSeconds(0);
		    currDate.setMilliseconds(0);

			if (mustBeFuture)
			{
				args.IsValid = (inputDate >= currDate);
			    if (!args.IsValid)
				{
			        setErrorMessage(source, errorPrefix, showErrorPrefixOnly, 'The date you have entered is in the past. Please enter a date of today or later');
				    txtBox.focus();
				    txtBox.select();
				    return;
				}
			}

			//now, check if date is too far in the future
			var futureDate = new Date();
		    futureDate.setFullYear(futureDate.getFullYear() + 10);
		    if(inputDate > futureDate)
		    {
				args.IsValid = false;
				setErrorMessage(source, errorPrefix, showErrorPrefixOnly, 'Input date is too far in the future');
				txtBox.focus();
				txtBox.select();
				return;
			}
		}

		// If input string is empty then no need to check further
		if (inputString == '')
		{
			args.IsValid = true;
			return;
		}

        // Comparison with other date input.
        hiddenFieldID = txtBox.id.replace('inputDate','ControlToCompare');
        hiddenField = document.getElementById(hiddenFieldID);
        if (hiddenField == null || hiddenField.value == '')
        {
            return;
        }
        var controlToCompareID = hiddenField.value + '_inputDate';
        var txtBoxToCompare = document.getElementById(controlToCompareID);
        if (txtBoxToCompare == null)
        {
            return;
        }

        hiddenFieldID = txtBox.id.replace('inputDate','Operator');
        hiddenField = document.getElementById(hiddenFieldID);
        if (hiddenField == null)
        {
            return;
        }
        var compareOperator = hiddenField.value;

        // Intialize variable 'dateToCompareName' to default value
        var dateToCompareName = controlToCompareID;
        hiddenFieldID = txtBox.id.replace('inputDate','ControlToCompareName');
        hiddenField = document.getElementById(hiddenFieldID);
        if (hiddenField != null)
        {
            dateToCompareName = hiddenField.value;
        }

        var controlToCompareChanged = (txtBoxToCompare.value != txtBoxToCompare.defaultValue);
        if (controlToValidateChanged ||controlToCompareChanged)
        {
            var compareToDate = new Date(txtBoxToCompare.value);
            var compToYear = compareToDate.getFullYear();
		    var compToMonth = compareToDate.getMonth() + 1;
		    var compToDay = compareToDate.getDate();
		    var condition = '';

		    if (!isNaN(compToYear) && !isNaN(compToMonth)&& !isNaN(compToDay))
		    {
		            if(compareOperator == 'DataTypeCheck')
                    {
                        return;
                    }
					//GreaterThan = later than
                    else if(compareOperator == 'must be not later than ')
                    {
                        args.IsValid = (inputDate > compareToDate);
                        condition = 'later than ';
                    }
					//GreaterThanEqual - not earlier than
                    else if (compareOperator == 'must be not earlier than ')
                    {
                        args.IsValid = (inputDate >= compareToDate);
                        condition = 'not earlier than ';
                    }
					//Equal - the same as
                    else if (compareOperator == 'Equal')
                    {
                        args.IsValid = (inputDate == compareToDate);
                        condition = 'must be the same as ';
                    }
					//LessThanEqual - not later than
                    else if (compareOperator == 'LessThanEqual')
                    {
                        args.IsValid = (inputDate <= compareToDate);
                        condition = 'must be not later than ';
                    }
					//LessThan - earlier than
                    else if (compareOperator == 'LessThan')
                    {
                        args.IsValid = (inputDate < compareToDate);
                        condition = 'must be earlier than ';
                    }
					//NotEqual - different from 
                    else if (compareOperator == 'NotEqual')
                    {
                        args.IsValid = (inputDate != compareToDate);
                        condition = 'must be different from ';
                    }


                if (!args.IsValid)
                {
			        setErrorMessage(source, errorPrefix, showErrorPrefixOnly, condition + dateToCompareName);
	                if (controlToValidateChanged)
	                {
	                    txtBox.focus();
	                    txtBox.select();
                    }
                    else
                    {
                        txtBoxToCompare.focus();
                        txtBoxToCompare.select();
                    }
                }
	            return;
		    }

        }

	}

	function setErrorMessage(source, errorPrefix, showErrorPrefixOnly, errorMessage)
	{
		var txtBox = document.getElementById(source.controltovalidate);

        var msg = errorPrefix + ((errorPrefix == '') ? '' : ' ') + (showErrorPrefixOnly ? '' : (errorMessage + '.'));
		source.errormessage = msg.replace('[x]', txtBox.value);
	}//]]>
</script>

