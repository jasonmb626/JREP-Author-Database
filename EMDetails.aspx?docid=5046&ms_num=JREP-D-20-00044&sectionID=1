	}
	var popupChangeControllingGroupWindow;
	function popupChangeControllingGroup()
	{
		var name = "DesignateControllingScheduleGroupSubmission.aspx?callPage=EMDetails.aspx&documentID=5046";
	    popupChangeControllingGroupWindow = openCenterWinRet(name, "popupChangeControllingGroupWindow", 1, 1, 0, 0, 0, 0);
	}

    
    function onAjaxCallbackPeopleCustomFlags(data)
    {
        var returnedData = JSON.parse(data);
        var classString = ".js-custompeopleflags-" + returnedData.PeopleID.toString();
        jQuery(classString).replaceWith(returnedData.FlagsHTML);
    }
	function onAjaxCallbackCustomFlagsOnly(requestResponseText)
	{
	    var customFlags = document.getElementById("customFlagsCell");
	    if(customFlags != null)
	    {
	        customFlags.innerHTML = requestResponseText;
	    }
	    var customFlagsRow = document.getElementById("customFlagsRow");
	    if(customFlagsRow != null && requestResponseText.length > 0)
	    {
	        customFlagsRow.style.display = "";
	    }
	    else if(customFlagsRow != null)
	    {
	        customFlagsRow.style.display = "none";
	    }
	}
	function onAjaxCallbackTechCheckFlagsOnly(requestResponseText)
	{
	    var techCheckFlag = document.getElementById("techCheckFlag");
	    if(techCheckFlag != null)
	    {
	        techCheckFlag.innerHTML = requestResponseText;
	    }
	}
    
    
    
    function UpdateSubmissionFlags (documentId, allCustomFlags) {
        
        
        if (allCustomFlags) { // current list of submission flags associated with the submission
            var customFlags = JSON.parse(allCustomFlags);
            
            // get all checkboxes representing the submission flags associated with the AMD fields
            var $chckAmdFlags = jQuery(':checkbox.amdAssociatedFlag');

            $chckAmdFlags.each(function(index, ele) {
                var $chkAmdFlag = jQuery(this),
                    flagId = $chkAmdFlag.attr('data-flagid'),
                    fieldId =  $chkAmdFlag.attr('data-fieldId'),
                    $hdnAmdWasChecked = jQuery('#hdnAMDWasChecked_' + fieldId),
                    turnedOn = false;

                jQuery.each(customFlags, function(i, flag) {
                    if (flag.FlagId == flagId) {
                        $hdnAmdWasChecked.val('True');
                        turnedOn = true;
                        return false;
                    }
                });

                // TT#31416 20170516 VP, reset the amd hiddenfield if the flag was not selected
                if (!turnedOn) {
                    $hdnAmdWasChecked.val(''); 
                }
                // turn on the AMD associated flag if it is in the current list of submission flags 
                // associated with the submission
                $chkAmdFlag.prop('checked', turnedOn);
            });
        
            
            $(".customQuestionAnswerFlag").each(function(index, ele) {                
                var foundFlag = false;
                var customFlagID = ele.id.substring(25);               

                
                jQuery.each(customFlags, function(i, flag) {                    
                    if (flag.FlagId == customFlagID) {
                        foundFlag = true;                        
                    }
                });

                
                if (!foundFlag) {                                    
                    $("#customQuestionAnswerFlag_" + customFlagID).remove();
                }

            });            

        }
        var url = "ajaxRequest.aspx";
		var querystring = "scheme=16&docId=" + documentId + "&flagType=";

		// 33035 20180621 MVK - replaced flagType parameter with value that indicates what type of flags to display,
		// which is based on which tab of the application is active.
		xmlHTTPPost(onAjaxCallbackCustomFlagsOnly, url, querystring + 1); 
		xmlHTTPPost(onAjaxCallbackTechCheckFlagsOnly, url, querystring + "2");    
	}

    
    function UpdatePeopleFlags(peopleId)
    {
        var url = "ajaxRequest.aspx";
        var querystring = "scheme=32&peopleId=" + peopleId;
        xmlHTTPPost(onAjaxCallbackPeopleCustomFlags, url, querystring);
    }
	function ViewExternalCorrespondence(externalCorrespondenceID, externalCorrespType, letterSecurityID)
	{
		var sNavUrl = "DotNetPopUps/ViewExternalCorrespondence.aspx";
		sNavUrl += "?hdnExternalCorrespondenceID=" + externalCorrespondenceID;
		sNavUrl += "&hdnExternalCorrespondenceType=" + externalCorrespType;
		sNavUrl += "&hdnLetterSecurityID=" + letterSecurityID;
		sNavUrl += "&hdnAssociatedEntityType=" + EXT_CORRES_ASSOC_TYPE_SUBMISSION;
			
		openCenterWin(sNavUrl, 'ViewExternalCorrespondence', 1, 1, 0, 0, 0, 0);
	}
	function VerifyProxy()
	{
        
		var querystring = "scheme=21&documentId=5046&manuscriptNumber=JREP-D-20-00044&sectionId=1";
		sendXmlHttpRequest(null, "ajaxRequest.aspx", querystring, false);
	}
    function refresh()
    {
        if (window.opener)
        {
            if (window.opener.refresh)
            {
                window.opener.refresh();
            }
            else
            {
                window.opener.location.reload();
            }
        }

        document.form1.submit();
    }
	function refreshOpener() {
		if (window.opener.detailsRefreshInvoked)
		{
		    // TT#25893    20130821    VP
		    // If this page is Popped up from AddEditScheduleGroup.aspx, we pass the Submission Specific values and Submission's Controlling Schedule Group specific values
		    // to the caller page, which uses these values to update the TOC.
		    if (False) {
		        var scheduleGroupToSubmissionMap = {};
		       
                scheduleGroupToSubmissionMap.DocumentID = 5046;
		        scheduleGroupToSubmissionMap.TargetNumberOfPages = '';
		        scheduleGroupToSubmissionMap.TargetStartPage = '';
		        scheduleGroupToSubmissionMap.TargetEndPage = '';
                scheduleGroupToSubmissionMap.TOCOrder = '0';
		        scheduleGroupToSubmissionMap.TargetOnlinePubDate = '';
		        scheduleGroupToSubmissionMap.ActualOnlinePubDate = '';

		        
		        scheduleGroupToSubmissionMap.BlackAndWhiteImageCount = '0'; 
		        scheduleGroupToSubmissionMap.ColorImageCount = '0';

		        scheduleGroupToSubmissionMap.DopplerBwCount = '';
		        scheduleGroupToSubmissionMap.DopplerColorCount = '';
		        scheduleGroupToSubmissionMap.NumFigures = '';
		        scheduleGroupToSubmissionMap.EstimatedFigurePages = '';
		        scheduleGroupToSubmissionMap.CharCountText = '';
		        scheduleGroupToSubmissionMap.TextManuscriptPages = '';
		        scheduleGroupToSubmissionMap.EstimatedTextPages = '';
		        scheduleGroupToSubmissionMap.NumReferences = '';
		        scheduleGroupToSubmissionMap.EstimatedReferencePages = '';
		        scheduleGroupToSubmissionMap.NumTables = '';
		        scheduleGroupToSubmissionMap.EstimatedTablePages = '';
		        scheduleGroupToSubmissionMap.EstimatedPrintedPages = '';
		        scheduleGroupToSubmissionMap.PreviousEditionChapter = '';
		        scheduleGroupToSubmissionMap.PreviousEditionPages = '';

		        window.opener.detailsRefreshInvoked(scheduleGroupToSubmissionMap);
		    } else {
		    window.opener.detailsRefreshInvoked();    
		    }
		}
        
		else if (window.opener.FGSubmissions && !(window.opener.detailsRefreshAvoidanceIndicator))
		{
			window.opener.location.href = window.opener.location.href;
		}
	}

    	
    function ResendApcPaymentRequest()
    {
        var okAction = function()
        {
            jQuery.ajax( {
                url: 'APCAsynchPaymentDueTaskHandler.ashx',
                type: 'post',
                data: { 'documentID': '5046' }
            } ).done( function ( response )
            {
                var rMsg = '';
                if ( response == 'fail' )
                {
                    rMsg = 'There was an unknown error.  Please contact the Editorial Office.';
                    $("#errorDialog")[0].showDialog( rMsg );
                }
                else
                {
                    rMsg = 'The Publication Charges Payment Request has been resent.';
                    $("#requestCompleteDialog")[0].showDialog( rMsg );
                }
                jQuery( '#linkApcStatus' ).css( 'display', 'none' );
            } );
        };
        $("#warningDialog")[0].showDialog('Are you sure you want to resend the Payment Request\x3f', okAction);
    }
    
	function updateMultipleScheduleGroupsDisplay(keyScheduleGroupId)
	{
		popupChangeControllingGroupWindow.close();
		jQuery.ajax({
			url: 'EMDetailsMultipleGroupsDisplay.ashx',
			type: 'post',
			data: {
				'documentID': '5046',
				'canManageScheduleGroups': 'False',
				'canViewScheduleGroups': 'False'
			}		
		}).done(function(response) {
			jQuery("#ScheduleGroupsTable").html(response);
		});
		if (jQuery("#TargetNumPages").length > 0) {
			jQuery("#TargetNumPages").html(sgData[keyScheduleGroupId].NumberOfPages);
		}
		if (jQuery("#ForControllingGroup").length > 0) {
			jQuery("#ForControllingGroup").html(FormatString(forControllingGroupString, [sgData[keyScheduleGroupId].ScheduleGroupName]));
		}
		if (jQuery("#ScheduleGroupTargetOnlinePublicationDate").length > 0) {
			jQuery("#ScheduleGroupTargetOnlinePublicationDate").html(sgData[keyScheduleGroupId].SGTargetOnlinePubDateFormatted);
		}
		if (jQuery("#ScheduleGroupTargetPublicationDate").length > 0) {
			jQuery("#ScheduleGroupTargetPublicationDate").html(sgData[keyScheduleGroupId].SGTargetPubDateFormatted);
		}
		if (jQuery("#ScheduleGroupTargetVolume").length > 0) {
			jQuery("#ScheduleGroupTargetVolume").html(sgData[keyScheduleGroupId].Volume);
		}
		if (jQuery("#ScheduleGroupTargetIssue").length > 0) {
			jQuery("#ScheduleGroupTargetIssue").html(sgData[keyScheduleGroupId].Issue);
		}
		if (jQuery("#txtPositionInScheduleGroupContents").length > 0) {
			jQuery("#txtPositionInScheduleGroupContents").val(sgData[keyScheduleGroupId].TocPosition);
		}
		if (jQuery("#txtTargetStartPage").length > 0) {
			jQuery("#txtTargetStartPage").val(sgData[keyScheduleGroupId].StartPage);
		}
		if (jQuery("#txtTargetEndPage").length > 0) {
			jQuery("#txtTargetEndPage").val(sgData[keyScheduleGroupId].EndPage);
		}
		if (jQuery("#ScheduleGroupNameLink").length > 0) {
			jQuery("#ScheduleGroupNameLink").text(sgData[keyScheduleGroupId].ScheduleGroupName);
			jQuery("#ScheduleGroupNameLink").attr("href", "javascript:popupScheduleGroupDetails(" + sgData[keyScheduleGroupId].ScheduleGroupId + ")");
		}
	}

	function PopUpFundingInformationWindow(url)
	{
		openCenterWinPercentage(url, 'fundingInfo', 1, 1, 0, 0, 1, 0, 800, 600, 0.5);
	}

    // handles window.focus for showing/hiding the CIF results link table cell, if necessary
	function WindowFocusEvent()
	{
	    var $CIFResultsLinkElements = $('.CIFResultsLinkElement');
	    var $CIFTopLink = $('a[href*="CIF"]');
	    CIFResultsLinkHandler_Initialize(CIFResultsLinkType_Submission, 5046);
	    var resultCount = CIFResultsLinkHandler_GetStatusCount();

	    if (resultCount > 0)
	    {
	        $CIFResultsLinkElements.show();
	        $CIFTopLink.show();
	    }
	    else
	    {
	        $CIFResultsLinkElements.hide();
	        $CIFTopLink.hide();
	    }
	}

