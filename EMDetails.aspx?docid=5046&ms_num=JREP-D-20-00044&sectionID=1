	var intFormHasChanged = 0;

	function DoProxyLogin(loginUrl)
	{
		VerifyProxy();
	    opener.location.href = loginUrl;
	}
	function formChanged()
	{
		intFormHasChanged = 1;
	}
	function abstractEditWarning(fieldName)
	{
	    $("#errorDialog")[0].showDialog(FormatString("Warning\x3a The {0} cannot be edited from Manuscript Details - any changes will not be saved. \x28Editors with \x27Edit Submission\x27 privileges may edit the abstract by clicking the \x27Edit Submission\x27 action link.\x29", [fieldName]));
	}
	function authorCommentEditWarning()
	{
	    $("#errorDialog")[0].showDialog("Warning\x3a Author Comments cannot be edited from  Manuscript Details - any changes will not be saved. \x28Editors with \x27Edit Submission\x27 privileges may edit Author Comments by clicking the \x27Edit Submission\x27 action link\x29.");
	}
	function getHelpTitle()
	{
		var strAdnlManuscriptDetails = "Additional Manuscript Details Field Definition";
		return strAdnlManuscriptDetails;
	}

	
	var textUpdateTimer;

	
	function updateTextarea(object, maxLen)
	{
		textUpdateTimer = setTimeout("updateTextarea2('" + object.id + "', " + maxLen + ");", 0);
	}
		
	function updateTextarea2(id, maxLen)
	{
		var object = document.getElementById(id);
		
		if (object.value.length > maxLen)
		{
			object.value = object.value.substr(0, maxLen);
		    $("#errorDialog")[0].showDialog(FormatString("There is a {0} character limit for this textarea.\n The text may have been truncated.", [maxLen]));
		}
	}
	function popupChargeFee(docID)
